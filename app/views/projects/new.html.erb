<div class="container">
  <div class=information>
    <h3>Project新規登録</h3>
    <div class="row">

      <%= form_with(model: @project, local: true) do |f| %>
        <div class="form-group">
          <%= f.label :name, 'Project Name' %>
          <%= f.text_field :name, class: 'form-control' %>
        </div>

        <div class="form-group">
          <%= f.label :customer, 'Customer Name' %>
          <%= f.text_field :customer, class: 'form-control' %>
        </div>


        <div class="form-group">
          <%= f.label :output, 'Document1' %>
          <%= f.text_field :output, class: 'form-control' %>
        </div>

        <%= f.submit '新規登録', class: 'btn btn-primary btn-block' %>   
      <% end %>

    </div>
    <p><%= link_to 'Project一覧に戻る', projects_path %></p>
  </div>
</div>