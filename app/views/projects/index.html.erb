<div class="container">
  <div class=information>
    <h3>Project一覧</h3>
    <h3><%= link_to 'Project新規登録', '/projects/new' %></h3>
    <table class="table table-striped table-bordered">
      <tr>
        <th class="text-center">Project Name</th>
        <th class="text-center">Customer Name</th>
        <th class="text-center">Document1</th>
      </tr>
      <% @projects.each do |project| %>
      <tr>
        <td class="text-center"><%= link_to project.name, project_path(project) %></td>
        <td class="text-center"><%= project.customer %></td>
        <td class="text-center"><%= project.output %></td>
      </tr>
      <% end %>
    </table>
    <p><%= link_to 'Projectを追加する', '/projects/new' %></p>
    <p><%= link_to '前のページに戻る', '/' %></p>
  </div>
</div>